###> doctrine/doctrine-bundle ###
# Valeurs fictives pour GitHub, seront remplacées par les Config Vars Heroku ou .env.local
MYSQL_USER=test_user
MYSQL_PASSWORD=test_password
MYSQL_DATABASE=test_db
DATABASE_URL="mysql://${MYSQL_USER}:${MYSQL_PASSWORD}@127.0.0.1:3306/${MYSQL_DATABASE}?serverVersion=8.0&charset=utf8mb4"
###< doctrine/doctrine-bundle ###

###> doctrine/mongodb-odm-bundle ###
# Valeurs fictives pour GitHub, remplacées par Config Vars Heroku ou .env.local
MONGO_INITDB_ROOT_USERNAME=test_mongo
MONGO_INITDB_ROOT_PASSWORD=test_mongo_pass
MONGO_DATABASE=test_mongo_db
MONGODB_URL="mongodb://${MONGO_INITDB_ROOT_USERNAME}:${MONGO_INITDB_ROOT_PASSWORD}@127.0.0.1:27017/${MONGO_DATABASE}?authSource=admin"
